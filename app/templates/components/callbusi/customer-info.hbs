                        <!-- <div class="inline-block form-input-group"><input class="input-group-content" type="date"></div> -->
                        <!-- <div class="inline-block form-input-group">{{input type="date" class="input-group-content"
        value=customerModel.birthdayString required="true" focus-out="changeBirthdayDateAction"}}</div> -->
                        <!-- <div class="inline-block form-input-group">{{input type="date" class="input-group-content"
          value=customerModel.birthdayString required="true"}}</div> -->
                        <div class="inline-block form-input-group">
                            <!-- {{bootstrap-datepicker placeholder="点击显示日历" class="form-control"
              format="yyyy-mm-dd" language="zh-CN" value=customerModel.birthdayTime
              changeDate="changeBirthdayAction"}} -->
                            {{ui/addon/bs-datetimepicker date=customerModel.birthdayTime placeholder=""
                            locale="zh-CN" iconClasses="fa fa-calendar" format='YYYY-MM-DD'
                            show="dpShowAction" change="changeBirthdayAction" }}
                        </div>
                    {{else}}
                        <div class="font-light input-group-content">{{customerModel.birthdayString}}</div>
                    {{/if}}
                </div>
            </div>
        </div>
        <div class="line">
            <div class="col-xs-12 col-sm-6 nopadding-left">
                <div class="form-input-group label-normal">
