<div class="busi-info">
    {{#bs-form formLayout=formLayout model=customerModel invalid=(action "invalid")}}
        <div class="col-xs-12 nopadding-horizontal top8">
            <div class="col-xs-4 img-font nopadding-left padding-bottom20"></div>
            {{#if showCustomer}}
            {{else}}
                <div class="col-xs-8 pull-right user-add-btn nopadding-horizontal">
                    {{#if shortSave}}
                    {{else}}
                        {{#if addMode}}
                            {{#if detailModify}}
                            {{else}}
                            {{/if}}
                        {{else}}
                            {{#if detailModify}}
                            {{else}}
                                {{#ui/icon-button iconClass="fa-edit pull-right incon-button-font" data-toggle="tooltip" title="编辑" data-placement="top" data-original-title="编辑" clickAction="detailModifyClick" detailModifyClick="detailModifyClick" }}
                                {{/ui/icon-button}}
                            {{/if}}
                        {{/if}}
                    {{/if}}
                    <div style="clear:both" />
                </div>
            {{/if}}
        </div>
        <div class="col-sm-12 nopadding-left bott cccc padding-bottom30">
            <div class="col-sm-6 nopadding-horizontal pull-left">
                {{#ui/img-intergration name="headImg" hasUploader=true showBigImg=true idNumber=idNumber imgSrc=customerInfo.avatarUrl height=100 width=100 margin-left=0 uploadUrl=uploadUrl uploadSucc="uploadSucc" uploadFail="uploadFailImg" businessType=constants.uploadBusinessType_headimg as |component|}}
                    <div class="pull-left photo-title {{if detailModify '' 'hidden'}}">
                        <div class="tip photo-title-color">
                            请上传入住老人的一张近照！
                        </div>
                        {{#if statusService.isJujia}}
                          <div class="tip photo-title-color margin-top-10">
                              上传图片大小需5M以内,支持png,jpg,jpeg.
                          </div>
                        {{/if}}
                        <div class="photo-footer">
                            <div class="button pull-left photo-footer-color" {{action 'avatarUpload' target=component on="click"}}>
                                上传图片
                            </div>
                        </div>
                    </div>
                {{/ui/img-intergration}}
            </div>
            {{#if statusService.isJujia}}
            {{else}}
            <div class="col-sm-6 nopadding-horizontal pull-left {{if customerInfo.qrCodeUrl '' 'hidden'}}">
                <div class="pull-left">
                    {{#ui/img-intergration name="headImg" showBigImg=true idNumber=idNumber imgSrc=customerInfo.realUrl height=110 width=110 margin-left=7}}
                    {{/ui/img-intergration}}
                    <div class="center">点击查看大图</div>
                </div>
                <div class="" style="padding-top:40px;">
                    <div class="tip photo-title-color">
                        <!-- 可以使用康颐微信公众号登录扫描二维码！ -->
                        通过APP扫描二维码查看老人服务详情
                    </div>
                </div>
            </div>
            {{/if}}
        </div>
        <div style="clear:both" />
        <div class="bottom12 col-sm-12 nopadding-left">
            <strong class="font16 font-black">个人资料</strong>
        </div>
        <div class="line ">
            <div class="col-xs-12 col-sm-6 nopadding-left">
                <div class="form-input-group label-normal">
                    <span class="input-group-addon">老人姓名：</span>
                    {{#if detailModify}}
                        <!-- {{input class="input-group-content" value=customerInfo.name type="text"}} -->
                        {{bs-form-element property="name" class="inline-block form-input-group" controlType="name" placeholder="必填项"}}
                    {{else}}
                        <div class="font-light input-group-content">{{customerModel.name}}</div>
                    {{/if}}
                </div>
            </div>
            <div class="col-xs-12 col-sm-6">
                <div class="form-input-group label-normal">
                    <span class="input-group-addon">身份证号：</span>
                    {{#if detailModify}}
                        <!-- {{input class="input-group-content" value=customerInfo.cardCode type="text"}} -->
                        {{bs-form-element property="cardCode" class="inline-block form-input-group" controlType="cardCode" placeholder="必填项" focus-out='computedCardCode' computedCardCode='computedCardCode'}}
                    {{else}}
                        <div class="font-light input-group-content">{{customerModel.cardCode}}</div>
                    {{/if}}
                </div>
            </div>
        </div>
        <div class="line">
            <div class="col-xs-12 col-sm-6 nopadding-left">
                <div class="form-input-group label-normal">
                    <span class="input-group-addon">老人性别：</span>
                    {{#if detailModify}}
