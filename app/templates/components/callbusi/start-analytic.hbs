        <div id="myChartBlood" style="height:400px;" class="text-center"></div>
    </div>     -->
<div class="fullwidth nopadding-left nopadding-right {{if bigFlag 'changeLarge' 'changeSmall'}}"  name='containerId'>
    <div class="box border blue" style="padding:0px;margin-bottom:0px;{{if chartContainer '' 'overflow:scroll'}}; ">
        <div class="box-title" style="position:relative;height:30px;padding:5px 10px 5px;">
            <h4 class="font16">{{titleShow.typename}}</h4>
            <div class="tools hidden-xs" >
                <!-- <a href="#box-config" data-toggle="modal" class="config" {{action "setClick" on="click"}}>
                    <i class="fa fa-cog"></i>
                </a> -->
                {{#ui/icon-button iconClass="fa-cog right8"
                data-toggle="tooltip"  title="设置"  data-placement="top" data-original-title="设置"
                clickAction="setClick" setClick="setClick"}}
                {{/ui/icon-button}}
                <!-- <a href="javascript:;" class="reload" {{action "bigClick" on="click"}}>
                    <i class="{{if bigFlag 'fa fa-compress' 'fa fa-expand'}}"></i>
                </a> -->
                {{#if bigFlag}}
                {{#ui/icon-button iconClass="fa-compress"
                data-toggle="tooltip"  title="缩小"  data-placement="top" data-original-title="缩小"
                clickAction="bigClick" bigClick="bigClick"}}
                {{/ui/icon-button}}
                {{else}}
                {{#ui/icon-button iconClass="fa-expand"
                data-toggle="tooltip"  title="放大"  data-placement="top" data-original-title="放大"
                clickAction="bigClick" bigClick="bigClick"}}
                {{/ui/icon-button}}
                {{/if}}
            </div>
            <!-- 弹框设置 -->
            <div class="tools hidden-xs {{if listShow '' 'hidden'}}" style="position:absolute;right:0px;z-index:2;top:29px;" focus-out='setClick' >
                <ul style="width:100px;padding-left:0px;background-color:#fff;">
                    <li class="pointer hoverPon {{if chartContainer '' 'hidden'}}" style="border:1px solid #5ea5bd;height:40px;font-size:14px;text-align:center;line-height:40px;border-right:none;border-bottom:none;" {{action 'listClick' on='click'}}><a style="text-decoration:none;"><i class="fa fa-list-ul" style="padding-right:5px;"></i>显示列表</a></li>
                    <li class="pointer hoverPon {{if listContainer '' 'hidden'}}" style="border:1px solid #5ea5bd;height:40px;font-size:14px;text-align:center;line-height:40px;border-right:none;border-bottom:none;" {{action 'chartClick' on='click'}}><a style="text-decoration:none;"><i class="fa fa-bar-chart-o" style="padding-right:5px;"></i>显示图表</a></li>
                    <li class="pointer hoverPon"  style="border:1px solid #5ea5bd;height:40px;font-size:14px;text-align:center;line-height:40px;border-right:none;border-bottom:none;"><a style="text-decoration:none;"><i class="fa fa-file" style="padding-right:5px;"></i>导出文件</a></li>
                    <li class="pointer hoverPon" style="border:1px solid #5ea5bd;height:40px;font-size:14px;text-align:center;line-height:40px;border-right:none;"><a style="text-decoration:none;"><i class="fa fa-print" style="padding-right:5px;"></i>数据打印</a></li>
                </ul>
            </div>
        </div>
        <div class="box-body" style="padding-left:0px;pading-right:0px;">
            <div name='echartContainer' style="{{if bigFlag 'height:380px;' 'height:280px;'}} " class="text-center {{if chartContainer '' 'hidden'}}"></div>
            <div class="{{if listContainer '' 'hidden'}}">
                <table id="datatable1" cellpadding="0" cellspacing="0" border="0" class="datatable table table-bordered table-hover dataTable mt15 border-none" aria-describedby="datatable1_info">
                    <thead>
                        <tr role="row">
                            <th rowspan="1" colspan="1" width="" class="center noBorder-all">时间</th>
                            {{#each statItemAry as |statItem index|}}
                                <th rowspan="1" colspan="1" width="" class="center noBorder-all">{{statItem.index}}</th>
                            {{/each}}
                        </tr>
                    </thead>
                    <tbody role="alert" aria-live="polite" aria-relevant="all" class="" id='{{tbodyIdName}}'>

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
