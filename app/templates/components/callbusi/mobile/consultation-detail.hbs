                    {{#each consultinfoData.backVistInfos as |backvist|}}
                        <li class=" carousel color666 border-top-1 border-bottom-1" id="backvist{{backvist.id}}" {{action "toBackvist" backvist on="tap"}}>
                            <i class="profile-i"><img class="a-img" src="assets/images/mobile/settings.png"/></i>
                            {{#if backvist.remarkString}}
                                <span class="profile-span">{{backvist.createDateTimeString}} | {{backvist.remarkString}}</span>
                            {{else}}
                                <span class="profile-span">{{backvist.createDateTimeString}} </span>
                            {{/if}}
