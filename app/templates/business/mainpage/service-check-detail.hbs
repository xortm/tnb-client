
<div class="col-sm-12">
	<div class="box margin-middle max-width">
		<!-- BOX-BODY -->
		<div class="box-body">
			<div class="row">
				<div class="col-xs-12">
					<p class="user-add-btn">
            {{#ui/form-function removeAction="delById" delById="delById" editNone=true}}
            {{/ui/form-function}}
					</p>
				</div>
				<!-- 列表详细介绍 -->
				<div class="col-xs-12 nopadding-left">
					<div class="col-xs-12 nopadding-horizontal mt20">

						<div class="col-xs-12 col-sm-6">
							<div class="form-input-group">
							<span class="input-group-addon">检查人：</span>
								<div class="input-group-content">{{serviceCheckInfo.staff.name}}</div>
							</div>
						</div>
            <div class="col-xs-12 col-sm-6">
							<div class="form-input-group">
							<span class="input-group-addon">检查时间：</span>
								<div class="input-group-content">{{serviceCheckInfo.createTimeStr}}</div>
							</div>
						</div>
						<div class="col-xs-12 col-sm-6">
							<div class="form-input-group">
							<span class="input-group-addon">检查内容：</span>
							<div class="input-group-content">{{serviceCheckInfo.item.typename}}</div>
							</div>
						</div>
						<div class="col-xs-12 col-sm-12">
							<div class="form-input-group">
								<span class="input-group-addon">检查结果：</span>
								<div class="input-group-content">{{serviceCheckInfo.remark}}</div>
							</div>
						</div>
					</div>
				</div>
				<!-- 列表详细介绍 -->
			</div>
		</div>
		<!-- /BOX-BODY -->
	</div>
</div>


{{#if showpopInvitePassModal}}
	{{#bs-modal body=false footer=false header=false closeAction="invitation"}}
		{{#bs-modal-header}}
			<h3 class="modal-title">删除提示</h3>
		{{/bs-modal-header}}
		{{#bs-modal-body}}
			<h4 class="modal-title">是否删除该条记录？</h4>
		{{/bs-modal-body}}

		{{#bs-modal-footer as |footer|}}
			{{#bs-button action=(action "cancelPassSubmit" serviceCheckInfo) type="primary" disabled=prepaidDisabled}}确定{{/bs-button}}
			{{#bs-button action=(action "invitation") type="default"}}取消{{/bs-button}}
		{{/bs-modal-footer}}
	{{/bs-modal}}
{{/if}}
