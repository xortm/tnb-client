<div class="col-md-12">
  <!-- BOX -->
  <div class="box">
    <div class="box-body">
      <div id="datatable1_wrapper" class="dataTables_wrapper form-inline" role="grid">
      	<div class="row">
          <div class="user-add-btn col-md-3">
            {{#ui/mix-button class="user-btn pull-right" iconClass="fa-plus"  text='添加'  clickAction="toDetailPage" toDetailPage="toDetailPage"}}
            {{/ui/mix-button}}
          </div>
      	</div>
        <table id="datatable1" cellpadding="0" cellspacing="0" border="0"  class="datatable table table-bordered table-hover dataTable" aria-describedby="datatable1_info">
          <thead>
            <tr role="row">
							<th rowspan="1" colspan="1" width="">设备类型</th>
              <th rowspan="1" colspan="1" width="">备注</th>
              <th rowspan="1" colspan="1" width="">操作</th>
            </tr>
          </thead>
          <tbody role="alert" aria-live="polite" aria-relevant="all" class="">
            {{#each tenantList as |tenant|}}
						<tr {{action "toDetailPage" tenant on="click"}}>
							<td class="ellipsis">{{tenant.orgName}}</td>
              <td class="ellipsis hidden-xs">{{tenant.address}}</td>
							<td class="ellipsis">{{tenant.orgTel}}</td>
							<td class="ellipsis hidden-xs">{{tenant.legalPerson}}</td>
							<td class="ellipsis hidden-xs">{{tenant.status.typename}}</td>
              <td class="ellipsis hidden-xs">
                {{#ui/mix-button class="user-btn edit-btn"   text='设备类型分配' clickAction="toEquipTypePage" toEquipTypePage="toEquipTypePage"  }}
                {{/ui/mix-button}}
              </td>
						</tr>
            {{/each}}
          </tbody>
        </table>
        {{#if tenantList }}
            {{page-numbers content=tenantList showFL=true numPagesToShow=5}}
            {{else}}
                <h1 class="col-sm-12 center unTask"></h1>
        {{/if}}
      </div>
    </div>
  </div>
  <!-- /BOX -->
</div>
