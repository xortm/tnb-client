<div class="col-sm-12">
    <div class="box">
        <div class="box-body">
            <div id="datatable1_wrapper" class="dataTables_wrapper form-inline" role="grid">
                <div class="row">
                    <!--搜索-->
                    <div class="col-sm-9 nopadding-left">
                        <!-- {{callbusi/time-search class="inline-block" text='按预警时间搜索' dateShow=dateShow chooseDate=chooseDate search="search"
            submmit="submmit" emptied='emptied' changeBeginDateAction='changeBeginDateAction' changeEndDateAction='changeEndDateAction'}} -->
                    </div>
                    <div class="user-add-btn col-sm-3 pull-right nopadding-right">
                        {{#ui/mix-button class=" user-btn pull-right" iconClass="fa-plus" text='新增活动类型' clickAction="toDetailPage" toDetailPage="toDetailPage" style="padding-right:0px;margin-right:0px;" }}
                        {{/ui/mix-button}}
                    </div>
                    <!--搜索-->
                </div>
                <table id="datatable1" cellpadding="0" cellspacing="0" border="0" class="datatable table table-bordered table-hover dataTable mt15" aria-describedby="datatable1_info">
                    <thead>
                        <tr role="row">
                            <th rowspan="1" colspan="1" width="">活动类型名称</th>
                            <!-- <th rowspan="1" colspan="1" width="">活动内容</th>
                            <th rowspan="1" colspan="1" width="">活动类型</th> -->
                        </tr>
                    </thead>
                    {{#if dicttypeList}}
                        <tbody role="alert" aria-live="polite" aria-relevant="all" class="">
                            {{#each dicttypeList as |dicttype|}}
                                <tr class="gradeA odd" {{action "toDetailPage" dicttype on="click"}}>
                                    <td class=" ellipsis sorting_1">{{dicttype.typename}}</td>
                                    <!-- <td class=" ellipsis ">{{dicttype.contents}}</td>
                                    <td class=" ellipsis">{{dicttype.type.typename}}</td> -->
                                </tr>
                            {{/each}}
                        </tbody>
                    {{else}}
                        <div class="nodate">暂无数据</div>
                    {{/if}}
                </table>
                {{#if dicttypeList }}
                    {{page-numbers content=dicttypeList showFL=true numPagesToShow=5}}
                {{else}}
                    <h1 class="col-sm-12 center unTask"></h1>
                {{/if}}
            </div>
        </div>
    </div>
    <!-- /BOX -->
</div>
