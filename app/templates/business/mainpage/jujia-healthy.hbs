<div id="userJujiaHealthyContainer" class="mainContent">
  <div class="wrapperInner jujia-healthyBd">
{{#ui/mobile/gestures-container}}
<div class="jujia-container healthy">
  <section class="msg-box">
    <img src="assets/images/kangyi/doctor.jpg" alt="">
<!--    <p>健康大讲堂</p>
    <a href="#">点击进入健康大讲堂>></a>-->
  </section>
  <section class="jujia-catalog">
    <ul>
      <li class="archives" id="healthy-file" {{action 'switchPage' 'healthy-file' 'healthy-file' on="tap"}}>
        <i class="fa fa-folder prev"></i>
        <span class="">档案</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="monitor" id="healthy-check" {{action 'switchPage' 'healthy-check' 'healthy-check' on="tap"}}>
        <i class="fa fa-stethoscope prev"></i>
        <span class="">监测</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="plan" id="healthy-plan" {{action 'switchPage' 'healthy-plan' 'healthy-plan' on="tap"}}>
        <i class="fa fa-file-text-o prev"></i>
        <span class="">方案</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="report" id="healthy-report" {{action 'switchPage' 'healthy-report' 'healthy-report' on="tap"}}>
        <i class="fa fa-line-chart prev"></i>
        <span class="">报告</span>
        <i class="fa fa-angle-right next"></i>
      </li>
    </ul>
  </section>
</div>


{{/ui/mobile/gestures-container}}
</div>
</div>
{{ui/container/infinite-scroll moniteData="userJujiaMy" didRenderAction="didRender" didInsertElementAction="didInsertElement"}}
