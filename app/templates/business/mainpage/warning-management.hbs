<div class="col-sm-12">
  <div class="box">
    <div class="box-body">
    <div id="datatable1_wrapper" class="dataTables_wrapper form-inline" role="grid">
    	<div class="row">
        <!--搜索-->
        <div class="col-sm-9 nopadding-left">
            {{callbusi/time-search class="inline-block" text='按预警时间搜索' dateShow=dateShow chooseDate=chooseDate search="search"
            submmit="submmit" emptied='emptied' changeBeginDateAction='changeBeginDateAction' changeEndDateAction='changeEndDateAction'}}
          </div>
          <div class="user-add-btn col-sm-3 pull-right nopadding-right">
            {{#ui/mix-button class=" user-btn pull-right" iconClass="fa-plus"  text='新增预警'  clickAction="toDetailPage" toDetailPage="toDetailPage" style="padding-right:0px;margin-right:0px;"}}
            {{/ui/mix-button}}
      	  </div>

    	  <!--搜索-->
    	</div>
      <table id="datatable1" cellpadding="0" cellspacing="0" border="0"  class="datatable table table-bordered table-hover dataTable mt15" aria-describedby="datatable1_info">
        <thead>
          <tr role="row">
            <th rowspan="1" colspan="1" width="">用户姓名</th>
            <th rowspan="1" colspan="1" width="">床位号</th>
            <th rowspan="1" colspan="1" width="">预警时间</th>
            <th rowspan="1" colspan="1" width="">预警说明</th>
            <th rowspan="1" colspan="1" class="" width="">处理人</th>
        		<th rowspan="1" colspan="1" class="" width="">处理状态</th>
            <th rowspan="1" colspan="1" class="" width="">复位时间</th>
        		<th rowspan="1" colspan="1" class="" width="">处理时间</th>
          </tr>
        </thead>
        {{#if warningList}}
      <tbody role="alert" aria-live="polite" aria-relevant="all" class="">
          {{#each warningList as |warning|}}
          <tr class="gradeA odd" {{action "toDetailPage" warning on="click"}}>
            <td class=" ellipsis sorting_1">{{warning.caller.name}}</td>
            <td class=" ellipsis ">{{warning.bed.roomBedName}}</td>
            <td class=" ellipsis">{{warning.createTimeString}}</td>
            <td class=" ellipsis ">{{warning.reason}}</td>
            <td class="ellipsis">{{warning.operater.name}}</td>
      	    <td class="ellipsis">
          		<span class="ellipsis floatLeft width90">{{warning.flag.typename}}</span>
      	    </td>
            <td class=" ellipsis">{{warning.cancelTimeStr}}</td>
            <td class="">{{warning.operateTimeShortString}}</td>
      	  </tr>
          {{/each}}
       </tbody>
       {{else}}
       {{/if}}
        </table>
        {{#if warningList }}
            {{page-numbers content=warningList showFL=true numPagesToShow=5}}
            {{else}}
                <h1 class="col-sm-12 center unTask"></h1>
        {{/if}}
      </div>
    </div>
  </div>
  <!-- /BOX -->
</div>
