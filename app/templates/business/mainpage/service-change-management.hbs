                            {{callbusi/time-search class="inline-block" text='按创建日期搜索' dateShow=dateShow chooseDate=chooseDate search="search"
           submmit="submmit" emptied='emptied' changeBeginDateAction='changeBeginDateAction' changeEndDateAction='changeEndDateAction'}}
                        </div>
                    </div>
                    <div class="user-add-btn col-sm-3 pull-right nopadding-right">
                        {{#ui/mix-button class=" user-btn pull-right" iconClass="fa-plus" text='新增变更申请' clickAction="toDetailPage" toDetailPage="toDetailPage" }}
                        {{/ui/mix-button}}
                    </div>
                </div>
                        <table id="datatable1" cellpadding="0" cellspacing="0" border="0" class="datatable table table-bordered table-hover dataTable mt15" aria-describedby="datatable1_info">
                            <thead>
                                <tr role="row">
                                    <th rowspan="1" colspan="1" width="">创建日期</th>
                                    <th rowspan="1" colspan="1" width="">申请人</th>
                                    <th rowspan="1" colspan="1" width="">入住状态</th>
                                    <th rowspan="1" colspan="1" width="">变更前床位</th>
                                    <th rowspan="1" colspan="1" width="">变更后床位</th>
                                    <th rowspan="1" colspan="1" class="" width="">变更前餐饮等级</th>
                                    <th rowspan="1" colspan="1" class="" width="">变更后餐饮等级</th>
                                    <th rowspan="1" colspan="1" class="" width="">变更前护理价格</th>
                                    <th rowspan="1" colspan="1" class="" width="">变更后护理价格</th>
                                    <th rowspan="1" colspan="1" class="" width="">变更状态</th>
                                </tr>
                            </thead>
                            {{#if changeList}}
                            <tbody role="alert" aria-live="polite" aria-relevant="all" class="">
                                {{#each changeList as |change|}}
                                    {{log "change item" change}}
                                    <tr class="gradeA odd" {{action "toDetailPage" change on="click"}}>
                                        <td class=" ellipsis sorting_1">{{change.createDateString}}</td>
                                        <td class=" ellipsis ">{{change.customer.name}}</td>
                                        <td class=" ellipsis ">{{change.customer.customerStatus.typename}}</td>
                                        <td class=" ellipsis ">{{change.bedOld.allBedName}}</td>
                                        <td class=" ellipsis ">{{change.bedNew.allBedName}}</td>
                                        <td class=" ellipsis ">{{{change.foodLevelOld.typename}}}</td>
                                        <td class=" ellipsis ">{{{change.foodLevelNew.typename}}}</td>
                                        <td class=" ellipsis ">{{{change.projectPriceOld}}}</td>
                                        <td class=" ellipsis ">{{{change.projectPriceNew}}}</td>
                                        <td class=" ellipsis ">{{{change.status.typename}}}</td>
                                    </tr>
                                {{/each}}
                            </tbody>
                            {{else}}
                            <div class="nodate">暂无数据</div>
                            {{/if}}
                        </table>
                        {{#if changeList }}
                            {{page-numbers content=changeList showFL=true numPagesToShow=3}}
                        {{else}}
                            <h1 class="col-sm-12 center unTask"></h1>
                        {{/if}}


            </div>
