{{#ui/mobile/gestures-container}}
<div id="customerWarningContainer" class="mainContent border-top-1 {{if global_pageConstructure.showLoader 'hide' ''}}">
  <div class="wrapperInner">
  {{#if hbeaconwarningList}}
    <div class="bgcolor-white">
    {{log "hbeaconwarningList hbs" hbeaconwarningList}}
    {{#each hbeaconwarningList as |hbeaconwarning|}}
      {{#callbusi/mobile/customer-warning-list item=hbeaconwarning outerClass="col-sm-6 col-xs-12 height-auto"}}
      {{/callbusi/mobile/customer-warning-list}}
    {{/each}}
    </div>
  {{else}}
    <div class="nodate">暂无数据</div>
  {{/if}}



  </div>

</div>
{{/ui/mobile/gestures-container}}

{{ui/container/infinite-scroll moniteData="hbeaconwarningList"
didRenderAction="didRender" didInsertElementAction="didInsertElement"}}
