<div class="col-md-12">
	<div class="box margin-middle max-width">
		<!-- BOX-BODY -->
		<div class="box-body">
			{{#bs-form formLayout=formLayout model=buildModel invalid=(action "invalid")}}
			<div class="row">
				<div class="col-xs-12">
					<p class="user-add-btn">
						{{#if detailEdit}}
						{{else}}
							<!-- {{#ui/icon-button class="user-btn trash-btn pull-right incon-button-font" iconClass="fa fa-trash-o"  text='删除'  clickAction="delById" delById="delById" clickActParams=buildingInfo}}
								{{bs-tooltip title="删除" class="light-tooltip"}}
							{{/ui/icon-button}}
							{{#ui/icon-button class="user-btn edit-btn pull-right incon-button-font" iconClass="fa fa-edit"  text='编辑'  clickAction="detailEditClick" detailEditClick="detailEditClick"}}
								{{bs-tooltip title="编辑" class="light-tooltip"}}
			        {{/ui/icon-button}} -->
							{{#ui/form-function editAction="detailEditClick" detailEditClick="detailEditClick"
							removeAction="delById" delById="delById" clickActParams=customerflow}}
							{{/ui/form-function}}

						{{/if}}
					</p>
				</div>
				<!-- 列表详细介绍 -->
				<div class="col-xs-12">
					<div class="col-xs-12 nopadding-horizontal mt20">
            <div class="col-xs-12 col-sm-6">
							<div class="form-input-group">
								<span class="input-group-addon">楼宇名称：</span>
								{{#if detailEdit}}
									{{#if id}}
										{{bs-form-element property="name" class="inline-block form-input-group label-normal" placeholder='楼宇名称(必填)'}}
								{{else}}
									{{bs-form-element property="name" class="inline-block form-input-group label-normal" placeholder='楼宇名称'}}
										{{/if}}
									{{else}}
									<div class="input-group-content">{{buildingInfo.name}}</div>
								{{/if}}
							</div>
						</div>

						<div class="col-xs-12 col-sm-6">
							<div class="form-input-group">
								<span class="input-group-addon">楼宇编号：</span>
								{{#if detailEdit}}
									{{#if id}}
									{{bs-form-element property="seq" class="inline-block form-input-group label-normal" placeholder='楼宇编号(必填)'}}
								{{else}}
									{{bs-form-element property="seq" class="inline-block form-input-group label-normal" placeholder='楼宇编号'}}
									{{/if}}
									{{else}}
									<div class="input-group-content">{{buildingInfo.seq}}</div>
								{{/if}}
							</div>
						</div>
						<div class="col-sm-12 nopadding-left">
							<div class="col-xs-12 col-sm-3 nopadding-right">
								<div class="form-input-group">
									<span class="input-group-addon">地上层数：</span>
									{{#if detailEdit}}
									{{#if id}}
										{{bs-form-element property="upLandFloorNum" class="inline-block form-input-group label-normal" placeholder='地上层数(必填)'}}
									{{else}}
										{{bs-form-element property="upLandFloorNum" class="inline-block form-input-group label-normal" placeholder='地上层数'}}
										{{/if}}
										{{else}}
										<div class="input-group-content">{{buildingInfo.upLandFloorNum}}</div>
									{{/if}}
								</div>
							</div>

							<div class="col-xs-12 col-sm-3 nopadding-right">
								<div class="form-input-group">
									<span class="input-group-addon">地下层数：</span>
									{{#if detailEdit}}
									{{#if id}}
										{{bs-form-element property="downLandFloorNum" class="inline-block form-input-group label-normal" placeholder='地下层数(必填)'}}
									{{else}}
										{{bs-form-element property="downLandFloorNum" class="inline-block form-input-group label-normal" placeholder='地下层数'}}
										{{/if}}
										{{else}}
										<div class="input-group-content">{{buildingInfo.downLandFloorNum}}</div>
									{{/if}}
								</div>
							</div>

							<div class="col-xs-12 col-sm-5">
									{{#if detailEdit}}
										{{#if id}}
											{{input type="checkbox" id="check1" checked=cbState}}
											<label class="building-span line-height-30" for="check1">不生成含有数字4的楼层号</label>
										{{else}}
											{{input type="checkbox" id="check1" checked=cbState}}
											<label class="building-span line-height-30" for="check1">不生成含有数字4的楼层号</label>
										{{/if}}
									{{else}}
									<div class="form-input-group">
										<span class="input-group-addon">是否生成含有数字4的楼层号：</span>
										{{#if  (eq buildingInfo.create4Flag 0)}}
											<span class="building-span">生成</span>
										{{else if (eq buildingInfo.create4Flag 1)}}
											<span class="building-span">不生成</span>
										{{/if}}
									</div>
									{{/if}}
							</div>
						</div>



						<div class="col-xs-12 col-sm-12">
							<div class="form-input-group">
								<span class="input-group-addon">备&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;注：</span>
								{{#if detailEdit}}
									{{#if id}}
										{{textarea type="text" class="full-width inline-block form-input-group label-normal form-control" value=buildingInfo.remark}}
									{{else}}
											{{textarea type="text" class="full-width inline-block form-input-group label-normal form-control" value=buildingInfo.remark placeholder='备注'}}
									{{/if}}
								{{else}}
									<div class="input-group-content">{{buildingInfo.remark}}</div>
								{{/if}}
							</div>

						</div>
					</div>
				</div>
				<!-- 列表详细介绍 -->
			</div>
			<div class="padding-top30 center">
				<p class="text-center user-add-btn margin0auto inlineBlock">
					{{#if detailEdit}}
					{{#ui/mix-button class="user-btn edit-btn" iconClass="fa fa-check"  text='保存' clickParams=buildingInfo clickAction="saveBuilding" saveBuilding="saveBuilding"}}
					{{/ui/mix-button}}
					{{#ui/mix-button class="user-btn times-btn" iconClass="fa fa-mail-reply"  text='取消'  clickAction="detailCancel" detailCancel="detailCancel" }}
					{{/ui/mix-button}}
					{{/if}}
				</p>
			</div>
			{{/bs-form}}
		</div>
		<!-- /BOX-BODY -->
	</div>
</div>

