<div id="userJujiaActivityContainer" class="mainContent">
  <div class="wrapperInner jujia-activityBd">
{{#ui/mobile/gestures-container}}
<div class="jujia-container healthy-plan">
  <section class="msg-box">
    <img src="assets/images/kangyi/jujia-activity.jpg" alt="">
  </section>
  <section class="jujia-catalog">
    <ul>
      <li class="activity-schedule" id="activity-schedule" {{action 'switchPage' 'activity-schedule' 'activity-schedule' on="tap"}}>
        <i class="fa fa-calendar-o prev"></i>
        <span class="">日程</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="activity-order" id="activity-order" {{action 'switchPage' 'activity-order' 'activity-order' on="tap"}}>
        <i class="fa fa-clock-o prev"></i>
        <span class="">预约</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="activity-record" id="activity-record" {{action 'switchPage' 'activity-record' 'activity-record' on="tap"}}>
        <i class="fa fa-video-camera prev"></i>
        <span class="">记录</span>
        <i class="fa fa-angle-right next"></i>
      </li>
    </ul>
  </section>
</div>


{{/ui/mobile/gestures-container}}
</div>
</div>
{{ui/container/infinite-scroll moniteData="userJujiaMy" didRenderAction="didRender" didInsertElementAction="didInsertElement"}}
