<div id="userJujiaMyContainer" class="mainContent">
  <div class="wrapperInner jujia-Bd">
{{#ui/mobile/gestures-container}}
<div class="jujia-container member">
  <section class="msg-box" id="member-account" {{action 'switchPage' 'member-account' 'member-account' on="tap"}}>
    <i class="head-out glyphicon business-mine-img">
        {{#ui/img-intergration name="headImg"
        imgSrc=curCustomer.avatarUrl height=70 width=70
        businessType=constants.uploadBusinessType_headimg}}
        {{/ui/img-intergration}}
    </i>
    <div class="msg">
      <p class="name">{{curCustomer.name}}</p>
      <p class="tell">手机号：{{curCustomer.phone}}</p>
    </div>
    <p class="vip" style="background: url('../assets/images/kangyi/vip.png') no-repeat left center;">{{curCustomer.customerVipInfo.level.typename}}</p>
  </section>
  <section class="jujia-catalog">
    <ul>
      <li class="account" id="member-account" {{action 'switchPage' 'member-account' 'member-account' on="tap"}}>
        <i class="fa fa-user prev"></i>
        <span class="">账号</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="album" id="member-photo" {{action 'switchPage' 'member-photo' 'member-photo' on="tap"}}>
        <i class="fa fa-image prev"></i>
        <span class="">相册</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="consume" id="member-consume" {{action 'switchPage' 'member-consume' 'member-consume' on="tap"}}>
        <i class="fa fa-cny prev"></i>
        <span class="">消费</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="notice" id="member-notice" {{action 'switchPage' 'member-notice' 'member-notice' on="tap"}}>
        <i class="fa fa-comment-o prev"></i>
        <span class="">通知</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="feedback" id="member-feedback" {{action 'switchPage' 'member-feedback' 'member-feedback' on="tap"}}>
        <i class="fa fa-edit prev"></i>
        <span class="">反馈</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="account" id="jujia-password" {{action 'switchPage' 'jujia-password' 'jujia-password' on="tap"}}>
        <i class="fa fa-lock prev"></i>
        <span class="">修改密码</span>
        <i class="fa fa-angle-right next"></i>
      </li>
      <li class="account" id="jujia-logout" {{action 'logout' 'jujia-logout' on="tap"}}>
        <i class="fa fa-sign-out prev"></i>
        <span class="">退出登录</span>
        <i class="fa fa-angle-right next"></i>
      </li>
    </ul>
  </section>
</div>


{{/ui/mobile/gestures-container}}
</div>
</div>
{{ui/container/infinite-scroll moniteData=""
didRenderAction="didRender" didInsertElementAction="didInsertElement"}}
